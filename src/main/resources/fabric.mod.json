{
  "schemaVersion": 1,
  "id": "pythoncmdapi",
  "version": "${version}",

  "name": "Python Command API",
  "description": "Allows for Python scripts to call commands in Minecraft.",
  "authors": [
    "craftablescience",
    "sebasphere",
    "mMONTAGEe"
  ],
  "contact": {
    "homepage": "https://craftablescience.github.io/minecraft-cmd-python-api",
    "sources": "https://www.curseforge.com/minecraft/mc-mods/python-command-api"
  },

  "license": "GPL-v3",
  "icon": "assets/pythoncmdapi/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": ["io.github.craftablescience.pythoncmdapi.PythonCommandAPI"]
  },
  "mixins": [
    "modid.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.9.2",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "modmenu:api": true,
    "modmenu:clientsideOnly": false
  }
}
